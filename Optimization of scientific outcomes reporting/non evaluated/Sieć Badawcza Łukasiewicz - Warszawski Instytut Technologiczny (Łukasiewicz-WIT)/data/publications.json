na i drzwi, podłogi, ściany działowe, dach z pokryciem, a także instalacje (elektryczne, wodno-kanalizacyjne, grzewcze, w tym np. instalacje ogrzewania podłogowego). Wykończenie i wyposażenie modułów zależy od zamawianego standardu."}],"affiliations":[{"id":"5e709192878c28a04737eda8","uid":"bs2kih5z0NID4UjAUHeocPQ","uuid":"3e8f9ad6-6157-47db-8e90-d9ba6d2e57d6","name":"Sieć Badawcza Łukasiewicz - Instytut Ceramiki i Materiałów Budowlanych","affiliatedPersonIDs":["5e70929c878c28a04739929a","5e709295878c28a04739875a"],"parentId":"5e709192878c28a04737eda8","parentUuid":"3e8f9ad6-6157-47db-8e90-d9ba6d2e57d6","rootId":"5e709192878c28a04737eda8","rootUuid":"3e8f9ad6-6157-47db-8e90-d9ba6d2e57d6","type":"AUTHOR"},{"id":"5e70918b878c28a04737dee0","uid":"bRxy5gSgbqozy-hdBgxLVzQ","uuid":"37426958-56d9-43c8-bc31-ad58ef900e9f","name":"Sieć Badawcza Łukasiewicz - Warszawski Instytut Technologiczny (Łukasiewicz-WIT)","affiliatedPersonIDs":["5e709489878c28a0473bf34d"],"parentId":"5e70918b878c28a04737dee0","parentUuid":"37426958-56d9-43c8-bc31-ad58ef900e9f","rootId":"5e70918b878c28a04737dee0","rootUuid":"37426958-56d9-43c8-bc31-ad58ef900e9f","type":"AUTHOR"}],"mainLanguage":"pol","mainLanguageName":"polski","volume":"5","pagesFromTo":"61-62","articleEvaluation":{"indexedJournal":false,"isReview":true,"wosConference":false},"contributionOrder":{"AUTHOR":["5e709295878c28a04739875a","5e70929c878c28a04739929a","5e709489878c28a0473bf34d"]},"journal":{"objectId":"61b9abf1ad204581b62ebae5","mniswId":"201101","title":"Materiały Budowlane - Science","issn":"0137-2971","eissn":"2449-951X","metadataSource":"MINISTRY","documentHash":"b6a0296f-6483-4943-be45-a8435b83a38c"},"_version":4,"journalIssue":{"objectId":"66e1528afdbe832737743c75","year":"2024","publishedYear":2024,"number":"5","documentHash":"43c68fe9-ebd1-40d7-83ad-f59597b041c6"},"translation":false,"editors":[],"translators":[],"institutionProfile":true,"translationEditors":[],"keywords":[],"lastRefresh":"1747952396425"}]